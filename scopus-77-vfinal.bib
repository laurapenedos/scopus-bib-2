@ARTICLE{Mike2022,
author={Mike, K. and Hazzan, O.},
title={MACHINE LEARNING FOR NON-MAJOR DATA SCIENCE STUDENTS: A WHITE BOX APPROACH},
journal={Statistics Education Research Journal},
year={2022},
volume={21},
